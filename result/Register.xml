                <persName><forename>Else </forename><surname type="linked">Lasker-Schüler</surname>
                <addName type="pseudonym">Jussuf, Prinz</addName>
                <addName type="pseudonym" ref="#b_01">Prinz von Theben</addName>
                    <addName type="pseudonym">blauer Jaguar Jussuf</addName>
                    <addName type="pseudonym">Prinz und Matrose</addName>
